{
    "name": "mirko-pagliai/cakephp-recaptcha-mailhide",
    "description": "A plugin that allows you to hide email addresses using reCAPTCHA",
    "homepage": "https://github.com/mirko-pagliai/cakephp-recaptcha-mailhide",
    "type": "cakephp-plugin",
    "license": "AGPL",
    "authors": [{
        "name": "Mirko Pagliai",
        "email": "mirko.pagliai@gmail.com"
    }],
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "~3.2"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "2.*",
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "RecaptchaMailhide\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RecaptchaMailhide\\Test\\": "tests"
        }
    }
}
