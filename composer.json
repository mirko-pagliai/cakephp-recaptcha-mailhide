{
    "name": "mirko-pagliai/cakephp-recaptcha-mailhide",
    "description": "A CakePHP plugin that allows you to hide email addresses using reCAPTCHA",
    "homepage": "https://github.com/mirko-pagliai/cakephp-recaptcha-mailhide",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Mirko Pagliai",
            "email": "mirko.pagliai@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0",
        "ctlabvn/recaptcha": "^3.0",
        "mirko-pagliai/php-tools": "^1.3"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^4.0",
        "mirko-pagliai/me-tools": "^2.19.6",
        "phpunit/phpunit": "^8.0"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "ctlabvn/recaptcha",
                "version": "3.0.0",
                "source": {
                    "url": "https://github.com/ctlabvn/recaptcha.git",
                    "type": "git",
                    "reference": "3.0.0"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Recaptcha\\": "vendor/ctlabvn/recaptcha/src/",
            "RecaptchaMailhide\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/test_app/TestApp/",
            "RecaptchaMailhide\\Test\\": "tests"
        }
    }
}
